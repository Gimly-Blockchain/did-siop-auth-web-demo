{
  "name": "@types/onto-web-demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start-server": "cd onto-demo-server && yarn run start",
    "start-client": "cd onto-demo-client && yarn run start",
    "build-types": "cd @types/onto-demo-shared-types && yarn run build",
    "build-server": "cd onto-demo-server && yarn run build",
    "build-client": "cd onto-demo-client && yarn run build",
    "build": "yarn build-types && concurrently \"yarn start-server\" \"yarn start-client\"",
    "start": "concurrently \"yarn start-server\" \"yarn start-client\""
  },
  "dependencies": {
    "dotenv-flow": "^3.2.0"
  },
  "devDependencies": {
    "concurrently": "^1.0.0"
  }
}
